  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.55.1",
    "rollup-plugin-dts": "^6.3.0",
    "typescript": "^5.8.3"
  }
