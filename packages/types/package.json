{
  "name": "@bmb-inc/types",
  "description": "Common TypeScript types and schema definitions for BMB Inc projects.",
  "version": "0.0.364",
  "author": "BMB Inc",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
